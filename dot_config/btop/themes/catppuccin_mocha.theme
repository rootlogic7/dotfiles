# Main background, empty for terminal default, need to be empty if you want transparent background
theme[main_bg]="#1e1e2e"

# Main text color
theme[main_fg]="#cdd6f4"

# Title color for boxes
theme[title]="#cdd6f4"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#f5e0dc" # Rosewater Akzent

# Background color of selected item in processes box
theme[selected_bg]="#45475a"

# Foreground color of selected item in processes box
theme[selected_fg]="#f5e0dc" # Rosewater

# Color of inactive/disabled text
theme[inactive_fg]="#7f849c"

# Color of text appearing on top of graphs, i.e uptime and current network graph scaling
theme[graph_text]="#f5e0dc"

# Background color of the percentage meters
theme[meter_bg]="#45475a"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#f5e0dc"

# Cpu box outline color
theme[cpu_box]="#f5e0dc" # Rosewater Border

# Memory box outline color
theme[mem_box]="#a6e3a1"

# Net box outline color
theme[net_box]="#f9e2af"

# Processes box outline color
theme[proc_box]="#89b4fa"

# Box divider line and small box line color
theme[div_line]="#6c7086"

# Temperature graph colors
theme[temp_start]="#f9e2af"
theme[temp_mid]="#fab387"
theme[temp_end]="#f38ba8"

# CPU graph colors
theme[cpu_start]="#a6e3a1"
theme[cpu_mid]="#f9e2af"
theme[cpu_end]="#f38ba8"

# Mem/Disk free meter
theme[free_start]="#94e2d5"
theme[free_mid]="#94e2d5"
theme[free_end]="#a6e3a1"

# Mem/Disk cached meter
theme[cached_start]="#89b4fa"
theme[cached_mid]="#89b4fa"
theme[cached_end]="#89dceb"

# Mem/Disk available meter
theme[available_start]="#f9e2af"
theme[available_mid]="#f9e2af"
theme[available_end]="#fab387"

# Mem/Disk used meter
theme[used_start]="#f38ba8"
theme[used_mid]="#f38ba8"
theme[used_end]="#eba0ac"

# Download graph colors
theme[download_start]="#89b4fa"
theme[download_mid]="#89b4fa"
theme[download_end]="#89dceb"

# Upload graph colors
theme[upload_start]="#f5c2e7"
theme[upload_mid]="#f5c2e7"
theme[upload_end]="#cba6f7"
